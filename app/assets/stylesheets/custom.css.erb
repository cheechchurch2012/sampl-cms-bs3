/* hero-unit */

.hero-unit {
  padding: 60px;
  margin-bottom: 30px;
  background-image: url(<%= asset_path 'logo_ipse.png' %>);
  background-repeat: no-repeat;
  background-position: 20px 20px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  
  h2 {
    color: #004781;
    line-height: 1.3em;
  }
}